{
    "name": "owenvoke/unit3d",
    "type": "library",
    "description": "An API wrapper for the UNIT3D torrent tracker.",
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "guzzlehttp/guzzle": "^6.5"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.5",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "OwenVoke\\Unit3d\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OwenVoke\\Unit3d\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "analyse": "phpstan analyse"
    },
    "config": {
        "sort-packages": true
    }
}
